{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "[[workspace]]/content"
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.taxonomy.tags": [
    "docker",
    "docker compose",
    "تثبيت دوكر لينكس"
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "image",
          "name": "image",
          "type": "string"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        },
        {
          "title": "summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "docker",
            "docker compose",
            "Docker-compose",
            "دوكر",
            "مجتمع أسس"
          ]
        }
      ]
    }
  ]
}